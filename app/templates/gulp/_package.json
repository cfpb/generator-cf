{
  "name": "<%= slugname %>",
  "version": "0.1.0",
  "description": "<%= props.description %>",
  <% if (props.homepage) { %>"homepage": "<%= props.homepage %>",<% } %>
  "author": {
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
    "url": "<%= props.authorUrl %>"<% } %>
  },
  "repository": {
    "type": "<%= props.repoType %>",
    "url": "<%= props.repoUrl %>"
  },
  "scripts": {
    "start": "parallelshell 'http-server ./dist/ -p7000' 'gulp watch'"
  },
  "license": "<%= props.license %>",
  "keywords": [
    "<%= slugname %>"
  ],
  "dependencies": {<% for(var i=0; i<components.length; i++) { %>
    "<%= components[i].name %>": "^<%= components[i].ver %>",<% } %>
    "html5shiv": "3.7.3"
  },
  "devDependencies": {
    "browser-sync": "2.23.6",
    "del": "3.0.0",
    "gulp": "4.0.0",
    "gulp-autoprefixer": "5.0.0",
    "gulp-changed": "3.2.0",
    "gulp-concat": "2.6.1",
    "gulp-clean-css": "3.9.2",
    "gulp-eslint": "4.0.2",
    "gulp-header": "2.0.1",
    "gulp-imagemin": "4.1.0",
    "gulp-less": "4.0.0",
    "gulp-mq-remove": "0.0.2",
    "gulp-notify": "3.2.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "2.6.4",
    "gulp-util": "3.0.8",
    "http-server": "0.11.1",
    "jquery": "1.11.3",
    "minimist": "1.2.0",
    "parallelshell": "3.0.2",
    "pretty-hrtime": "1.0.3",
    "require-dir": "1.0.0",
    "uglifyjs-webpack-plugin": "1.2.2",
    "webpack": "3.10.0",
    "webpack-stream": "4.0.2"
  }
}
