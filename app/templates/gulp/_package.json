{
  "name": "<%= slugname %>",
  "version": "0.1.0",
  "description": "<%= props.description %>",
  <% if (props.homepage) { %>"homepage": "<%= props.homepage %>",<% } %>
  "author": {
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
    "url": "<%= props.authorUrl %>"<% } %>
  },
  "repository": {
    "type": "<%= props.repoType %>",
    "url": "<%= props.repoUrl %>"
  },
  "scripts": {
    "start": "parallelshell 'http-server ./dist/ -p7000' 'gulp watch'"
  },
  "license": "<%= props.license %>",
  "keywords": [
    "<%= slugname %>"
  ],
  "dependencies": {<% for(var i=0; i<components.length; i++) { %>
    "<%= components[i].name %>": "^<%= components[i].ver %>",<% } %>
    "html5shiv": "3.7.3"
  },
  "devDependencies": {
    "browser-sync": "2.18.12",
    "del": "3.0.0",
    "fs": "0.0.2",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "4.0.0",
    "gulp-changed": "3.1.0",
    "gulp-concat": "2.6.1",
    "gulp-clean-css": "3.7.0",
    "gulp-eslint": "3.0.1",
    "gulp-header": "1.8.9",
    "gulp-imagemin": "3.3.0",
    "gulp-less": "3.3.2",
    "gulp-mq-remove": "0.0.2",
    "gulp-notify": "3.0.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "2.6.0",
    "gulp-uglify": "3.0.0",
    "gulp-util": "3.0.8",
    "http-server": "0.10.0",
    "jquery": "1.11.3",
    "minimist": "1.2.0",
    "parallelshell": "3.0.1",
    "pretty-hrtime": "1.0.3",
    "require-dir": "0.3.2",
    "webpack": "3.2.0",
    "webpack-stream": "3.2.0"
  }
}
