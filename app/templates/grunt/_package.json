{
  "name": "<%= slugname %>",
  "version": "0.1.0",
  "description": "<%= props.description %>",
  <% if (props.homepage) { %>"homepage": "<%= props.homepage %>",<% } %>
  "author": {
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
    "url": "<%= props.authorUrl %>"<% } %>
  },
  "repository": {
    "type": "<%= props.repoType %>",
    "url": "<%= props.repoUrl %>"
  },
    "scripts": {
    "start": "parallelshell 'http-server ./dist/ -p7000' 'grunt watch'"
  },
  "license": "<%= props.license %>",
  "keywords": [
    "<%= slugname %>"
  ],
  "dependencies": {<% for(var i=0; i<components.length; i++) { %>
    "<%= components[i].name %>": "^<%= components[i].ver %>",<% } %>
    "html5shiv": "3.7.3",
    "jquery": "1.12.2"
  },
  "devDependencies": {
    "grunt": "^1.0.1",
    "grunt-autoprefixer": "^3.0.4",
    "grunt-banner": "^0.6.0",
    "grunt-browserify": "^5.0.0",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.0",
    "grunt-contrib-less": "^1.4.1",
    "grunt-contrib-uglify": "^3.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-legacssy": "0.4.0",
    "grunt-string-replace": "^1.3.1",
    "grunt-topdoc": "0.3.0",
    "http-server": "^0.10.0",
    "jit-grunt": "^0.10.0",
    "parallelshell": "^3.0.1",
    "time-grunt": "^1.4.0"
  },
  "scripts": {
    "test": "grunt test"
  }
}
